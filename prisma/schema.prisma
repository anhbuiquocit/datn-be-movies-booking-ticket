generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id        Int       @id @default(autoincrement())
  createAt  DateTime  @default(now())
  updateAt  DateTime? 
  deleteAt  DateTime?
  firstname String
  lastname  String
  age       Int
  address   String
  email     String    @unique
  password  String
  username  String    @unique
}
